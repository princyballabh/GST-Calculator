# Environment Variables Template
# Copy this file to .env and fill in your actual values

# MongoDB Connection String
# Get this from MongoDB Atlas or use local: mongodb://localhost:27017/
MONGO_URL=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0

# Admin Panel Password
# Set a secure password for admin access
ADMIN_KEY=your_secure_admin_password_here

# Optional: Database Name (defaults to 'gst_calculator')
# DB_NAME=gst_calculator

# Optional: Enable Debug Mode (defaults to False)
# DEBUG=True
